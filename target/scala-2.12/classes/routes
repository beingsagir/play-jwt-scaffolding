# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                               controllers.HomeController.index
GET     /signUp/:email/:password        controllers.HomeController.signUp(email: String, password: String)
GET     /signIn/:email/:password        controllers.HomeController.signIn(email: String, password: String)


#authentication test
GET     /authCheck                              controllers.HomeController.access

#Auth less data access
GET     /userCreatedTest/:email/:password       controllers.HomeController.getCurrentUserForTest(email: String, password: String)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file                   controllers.Assets.versioned(path="/public", file: Asset)
